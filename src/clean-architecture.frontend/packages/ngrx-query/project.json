{
  "projectType": "library",
  "root": "packages/ngrx-query",
  "sourceRoot": "packages/ngrx-query/src",
  "prefix": "clean-architecture.frontend",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/ngrx-query"],
